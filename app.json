{
  "expo": {
    "name": "Piggy",
    "slug": "Piggy",
    "scheme": "Piggy",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/appAssets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appAssets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appAssets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/appAssets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}